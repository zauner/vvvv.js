interpolate(200 0, 250):
         -0.5  ->  175
          0.0  ->  200
          0.5  ->  225
          1.0  ->  250
          1.5  ->  275

interpolateString(200, 250.00):
         -0.5  ->  175
          0.0  ->  200
          0.5  ->  225
          1.0  ->  250
          1.5  ->  275

interpolateNumber(200, 250):
         -0.5  ->  175
          0.0  ->  200
          0.5  ->  225
          1.0  ->  250
          1.5  ->  275

interpolateObject({ key: 200 }, { key: 250 }):
         -0.5  ->  { key: 175 }
          0.0  ->  { key: 200 }
          0.5  ->  { key: 225 }
          1.0  ->  { key: 250 }
          1.5  ->  { key: 275 }

interpolateArray([ 200 ], [ 250 ]):
         -0.5  ->  [ 175 ]
          0.0  ->  [ 200 ]
          0.5  ->  [ 225 ]
          1.0  ->  [ 250 ]
          1.5  ->  [ 275 ]

interpolateRgb(red, blue):
         -0.5  ->  rgb(383,0,-127)
          0.0  ->  rgb(255,0,0)
          0.5  ->  rgb(128,0,128)
          1.0  ->  rgb(0,0,255)
          1.5  ->  rgb(-127,0,383)

interpolateHsl(red, blue):
         -0.5  ->  #0000ff
          0.0  ->  #ff0000
          0.5  ->  #00ff00
          1.0  ->  #0000ff
          1.5  ->  #ff0000

